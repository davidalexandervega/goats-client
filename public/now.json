{
  "version": 2,
  "name": "goatsguide",
  "alias": [
    "goatsguide.com",
    "www.goatsguide.com"
  ],
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ],
  "builds": [
    {
      "src": "favicon.ico",
      "use": "@now/static"
    }
  ]
}
